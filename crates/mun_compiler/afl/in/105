pub fn equals(a: i16, b: i16) -> bool { a == b }
pub fn not_equal(a: i16, b: i16) -> bool { a != b}
pub fn less(a: i16, b: i16) -> bool { a < b }
pub fn less_equal(a: i16, b: i16) -> bool { a <= b }
pub fn greater(a: i16, b: i16) -> bool { a > b }
pub fn greater_equal(a: i16, b: i16) -> bool { a >= b }