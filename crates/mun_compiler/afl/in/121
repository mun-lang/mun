pub fn assign(a: uint, b: uint) -> uint {
    a = b;
    a
}
pub fn assign_add(a: uint, b: uint) -> uint {
    a += b;
    a
}
pub fn assign_subtract(a: uint, b: uint) -> uint {
    a -= b;
    a
}
pub fn assign_multiply(a: uint, b: uint) -> uint {
    a *= b;
    a
}
pub fn assign_divide(a: uint, b: uint) -> uint {
    a /= b;
    a
}
pub fn assign_remainder(a: uint, b: uint) -> uint {
    a %= b;
    a
}