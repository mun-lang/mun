pub fn equals(a: u128, b: u128) -> bool { a == b }
pub fn not_equal(a: u128, b: u128) -> bool { a != b}
pub fn less(a: u128, b: u128) -> bool { a < b }
pub fn less_equal(a: u128, b: u128) -> bool { a <= b }
pub fn greater(a: u128, b: u128) -> bool { a > b }
pub fn greater_equal(a: u128, b: u128) -> bool { a >= b }