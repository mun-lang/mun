pub fn assign(a: u64, b: u64) -> u64 {
    a = b;
    a
}
pub fn assign_add(a: u64, b: u64) -> u64 {
    a += b;
    a
}
pub fn assign_subtract(a: u64, b: u64) -> u64 {
    a -= b;
    a
}
pub fn assign_multiply(a: u64, b: u64) -> u64 {
    a *= b;
    a
}
pub fn assign_divide(a: u64, b: u64) -> u64 {
    a /= b;
    a
}
pub fn assign_remainder(a: u64, b: u64) -> u64 {
    a %= b;
    a
}