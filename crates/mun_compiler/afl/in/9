pub fn equals(a: f64, b: f64) -> bool { a == b }
pub fn not_equal(a: f64, b: f64) -> bool { a != b}
pub fn less(a: f64, b: f64) -> bool { a < b }
pub fn less_equal(a: f64, b: f64) -> bool { a <= b }
pub fn greater(a: f64, b: f64) -> bool { a > b }
pub fn greater_equal(a: f64, b: f64) -> bool { a >= b }