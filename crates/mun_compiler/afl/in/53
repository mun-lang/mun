pub fn assign(a: u8, b: u8) -> u8 {
    a = b;
    a
}
pub fn assign_add(a: u8, b: u8) -> u8 {
    a += b;
    a
}
pub fn assign_subtract(a: u8, b: u8) -> u8 {
    a -= b;
    a
}
pub fn assign_multiply(a: u8, b: u8) -> u8 {
    a *= b;
    a
}
pub fn assign_divide(a: u8, b: u8) -> u8 {
    a /= b;
    a
}
pub fn assign_remainder(a: u8, b: u8) -> u8 {
    a %= b;
    a
}