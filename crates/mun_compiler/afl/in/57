pub fn assign(a: u16, b: u16) -> u16 {
    a = b;
    a
}
pub fn assign_add(a: u16, b: u16) -> u16 {
    a += b;
    a
}
pub fn assign_subtract(a: u16, b: u16) -> u16 {
    a -= b;
    a
}
pub fn assign_multiply(a: u16, b: u16) -> u16 {
    a *= b;
    a
}
pub fn assign_divide(a: u16, b: u16) -> u16 {
    a /= b;
    a
}
pub fn assign_remainder(a: u16, b: u16) -> u16 {
    a %= b;
    a
}