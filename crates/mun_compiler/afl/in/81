pub fn assign(a: f64, b: f64) -> f64 {
    a = b;
    a
}
pub fn assign_add(a: f64, b: f64) -> f64 {
    a += b;
    a
}
pub fn assign_subtract(a: f64, b: f64) -> f64 {
    a -= b;
    a
}
pub fn assign_multiply(a: f64, b: f64) -> f64 {
    a *= b;
    a
}
pub fn assign_divide(a: f64, b: f64) -> f64 {
    a /= b;
    a
}
pub fn assign_remainder(a: f64, b: f64) -> f64 {
    a %= b;
    a
}