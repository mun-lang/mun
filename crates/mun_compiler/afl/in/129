pub fn equals(a: uint, b: uint) -> bool { a == b }
pub fn not_equal(a: uint, b: uint) -> bool { a != b}
pub fn less(a: uint, b: uint) -> bool { a < b }
pub fn less_equal(a: uint, b: uint) -> bool { a <= b }
pub fn greater(a: uint, b: uint) -> bool { a > b }
pub fn greater_equal(a: uint, b: uint) -> bool { a >= b }