pub fn assign(a: f32, b: f32) -> f32 {
    a = b;
    a
}
pub fn assign_add(a: f32, b: f32) -> f32 {
    a += b;
    a
}
pub fn assign_subtract(a: f32, b: f32) -> f32 {
    a -= b;
    a
}
pub fn assign_multiply(a: f32, b: f32) -> f32 {
    a *= b;
    a
}
pub fn assign_divide(a: f32, b: f32) -> f32 {
    a /= b;
    a
}
pub fn assign_remainder(a: f32, b: f32) -> f32 {
    a %= b;
    a
}