pub fn assign(a: u128, b: u128) -> u128 {
    a = b;
    a
}
pub fn assign_add(a: u128, b: u128) -> u128 {
    a += b;
    a
}
pub fn assign_subtract(a: u128, b: u128) -> u128 {
    a -= b;
    a
}
pub fn assign_multiply(a: u128, b: u128) -> u128 {
    a *= b;
    a
}
pub fn assign_divide(a: u128, b: u128) -> u128 {
    a /= b;
    a
}
pub fn assign_remainder(a: u128, b: u128) -> u128 {
    a %= b;
    a
}